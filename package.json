		"next": "15.1.4",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-google-recaptcha": "^3.1.0",
		"react-hook-form": "^7.54.2",
		"react-hot-toast": "^2.5.1",
		"react-icons": "^5.4.0",
		"react-intersection-observer": "^9.15.0",
		"react-markdown": "^9.0.3",
		"react-select": "^5.10.1",
		"zod": "^3.24.1",
		"zustand": "^5.0.3"
	},
