		"@types/react-google-recaptcha": "^2.1.9",
		"@types/testing-library__jest-dom": "^6.0.0",
		"@vitejs/plugin-react": "^4.3.4",
		"eslint": "^9.17.0",
		"eslint-config-next": "15.1.4",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jsdom": "^26.0.0",
		"pa11y": "^8.0.0",
